<table class="table">
  <tbody>
      <tr>
        <tr><%= image_tag user.get_image(100,100), class:'rounded-circle' %></tr>
      </tr>
      <tr>
        <th>name</th>
        <td><%= user.name %></td>
      </tr>
      <tr>
        <th>introduction</th>
        <td><%= user.introduction %></td>
      </tr>
      <tr>
        <th>
         <!--ユーザー編集ボタン-->
         <%= link_to edit_user_path(current_user.id),class:"btn btn-outline-secondary btn-block" do %>
            <i class="fas fa-user-cog"></i>
          <% end %>
        </th>
      </tr>
  </tbody>
</table>